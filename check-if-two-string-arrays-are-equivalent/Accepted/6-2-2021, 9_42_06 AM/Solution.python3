// https://leetcode.com/problems/check-if-two-string-arrays-are-equivalent

class Solution:
    def arrayStringsAreEqual(self, word1: List[str], word2: List[str]) -> bool:
        out1 = ''
        out2 = ''
        out1 = out1.join(word1)
        out2 = out2.join(word2)
        return out1==out2
        